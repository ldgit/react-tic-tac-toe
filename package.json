{
  "name": "tic-tac-toe-react",
  "version": "1.0.0",
  "description": "React intro tutorial (https://reactjs.org/tutorial/tutorial.html)",
  "main": "index.js",
  "homepage": "https://ldgit.github.io/react-tic-tac-toe/",
  "scripts": {
    "test": "nyc --reporter=text mocha --require @babel/register",
    "test:watch": "argus",
    "coverage": "npm t && nyc report --reporter=text-lcov | coveralls",
    "build": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "start": "webpack-dev-server --open --mode=development",
    "lint": "eslint --ext .jsx,.js ./",
    "test:all": "npm run lint &&  npm t",
    "deploy": "npm run test:all && npm run build:prod && rm -rf docs/* && cp -R dist/. docs/"
  },
  "author": "Danko Lučić",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldgit/react-tic-tac-toe.git"
  },
  "dependencies": {
    "node-sass": "^4.9.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "sass-loader": "^7.1.0"
  },
  "nyc": {
    "extension": [
      ".jsx"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "argus-test-runner": "^2.0.0",
    "babel-loader": "^8.0.2",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^12.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  }
}
